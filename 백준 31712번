#include<iostream>
using namespace std;

int main() {
	int arr[3][3], hp, second = -1;

	for (int i = 0; i < 3; i++) {
		for (int j = 0; j <= 1; j++)
			cin >> arr[i][j];
	}cin >> hp;

	while (hp > 0)
	{
		second++;
		if (second % arr[0][0] == 0)
			hp -= arr[0][1];
		if (second % arr[1][0] == 0)
			hp -= arr[1][1];
		if (second % arr[2][0] == 0)
			hp -= arr[2][1];
	}cout << second << endl;

	
	return 0;
}
